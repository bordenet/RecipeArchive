    "title": "Perfect Chocolate Chip Cookies",
    "ingredients": [
      {
        "text": "2¼ cups all-purpose flour",
        "amount": 2.25,
        "unit": "cups",
        "ingredient": "all-purpose flour"
      },
      {
        "text": "1 teaspoon baking soda",
        "amount": 1,
        "unit": "teaspoon",
        "ingredient": "baking soda"
      },
      {
        "text": "1 teaspoon salt",
        "amount": 1,
        "unit": "teaspoon",
        "ingredient": "salt"
      },
      {
        "text": "1 cup (2 sticks) butter, softened",
        "amount": 1,
        "unit": "cup",
        "ingredient": "butter, softened"
      },
      {
        "text": "¾ cup granulated sugar",
        "amount": 0.75,
        "unit": "cup",
        "ingredient": "granulated sugar"
      },
      {
        "text": "¾ cup packed brown sugar",
        "amount": 0.75,
        "unit": "cup",
        "ingredient": "packed brown sugar"
      },
      {
        "text": "2 large eggs",
        "amount": 2,
        "unit": "large",
        "ingredient": "eggs"
      },
      {
        "text": "2 teaspoons vanilla extract",
        "amount": 2,
        "unit": "teaspoons",
        "ingredient": "vanilla extract"
      },
      {
        "text": "2 cups chocolate chips",
        "amount": 2,
        "unit": "cups",
        "ingredient": "chocolate chips"
      }
    ],
    "instructions": [
      {
        "stepNumber": 1,
        "text": "Preheat oven to 375°F (190°C)."
      },
      {
        "stepNumber": 2,
        "text": "In a medium bowl, combine flour, baking soda and salt; set aside."
      },
      {
        "stepNumber": 3,
        "text": "In a large bowl, beat butter and both sugars until light and fluffy, about 3 minutes."
      },
      {
        "stepNumber": 4,
        "text": "Beat in eggs one at a time, then vanilla."
      },
      {
        "stepNumber": 5,
        "text": "Gradually mix in flour mixture until just combined."
      },
      {
        "stepNumber": 6,
        "text": "Stir in chocolate chips."
      },
      {
        "stepNumber": 7,
        "text": "Drop rounded tablespoons of dough onto ungreased cookie sheets, spacing 2 inches apart."
      },
      {
        "stepNumber": 8,
        "text": "Bake 9-11 minutes until golden brown around edges. Cool on baking sheet 2 minutes before transferring to wire rack."
      }
    ],
