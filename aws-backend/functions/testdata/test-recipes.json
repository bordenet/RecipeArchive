    "title": "Grandma's Chicken Noodle Soup",
    "ingredients": [
      {
        "text": "1 whole chicken (3-4 lbs)",
        "amount": 1,
        "unit": "whole",
        "ingredient": "chicken"
      },
      {
        "text": "12 cups water",
        "amount": 12,
        "unit": "cups",
        "ingredient": "water"
      },
      {
        "text": "2 bay leaves",
        "amount": 2,
        "unit": "leaves",
        "ingredient": "bay leaves"
      },
      {
        "text": "1 large onion, diced",
        "amount": 1,
        "unit": "large",
        "ingredient": "onion, diced"
      },
      {
        "text": "3 carrots, sliced",
        "amount": 3,
        "unit": "carrots",
        "ingredient": "carrots, sliced"
      },
      {
        "text": "3 celery stalks, chopped",
        "amount": 3,
        "unit": "stalks",
        "ingredient": "celery stalks, chopped"
      },
      {
        "text": "8 oz wide egg noodles",
        "amount": 8,
        "unit": "oz",
        "ingredient": "wide egg noodles"
      },
      {
        "text": "2 tablespoons fresh parsley, chopped",
        "amount": 2,
        "unit": "tablespoons",
        "ingredient": "fresh parsley, chopped"
      },
      {
        "text": "Salt and pepper to taste",
        "amount": null,
        "unit": "to taste",
        "ingredient": "salt and pepper"
      }
    ],
    "instructions": [
      {
        "stepNumber": 1,
        "text": "In a large stock pot, combine chicken, water, and bay leaves. Bring to a boil."
      },
      {
        "stepNumber": 2,
        "text": "Reduce heat and simmer for 1 hour until chicken is tender and cooked through."
      },
      {
        "stepNumber": 3,
        "text": "Remove chicken from pot and let cool. Remove and discard skin and bones, then shred meat into bite-sized pieces."
      },
      {
        "stepNumber": 4,
        "text": "Strain the broth and return to pot. Bring to a simmer."
      },
      {
        "stepNumber": 5,
        "text": "Add onion, carrots, and celery to the broth. Cook for 15 minutes until vegetables are tender."
      },
      {
        "stepNumber": 6,
        "text": "Add egg noodles and cook according to package directions, about 8-10 minutes."
      },
      {
        "stepNumber": 7,
        "text": "Return shredded chicken to pot and heat through, about 5 minutes."
      },
      {
        "stepNumber": 8,
        "text": "Remove bay leaves. Season with salt, pepper, and fresh parsley before serving."
      }
    ],
