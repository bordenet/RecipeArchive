# Extension Environment Configuration Template
# Copy this file to .env and update with your actual values
# NEVER commit the .env file to version control

# Note: Browser extensions use localStorage for configuration
# Use the setup script or manually set these values in browser console

# AWS Cognito Configuration
COGNITO_USER_POOL_ID=your_user_pool_id_here
COGNITO_APP_CLIENT_ID=your_app_client_id_here
AWS_REGION=us-west-2

# API Configuration
API_BASE_URL=your_api_gateway_url_here

# Test User Credentials (for development/testing only)
TEST_USER_EMAIL=your_test_email_here
TEST_USER_PASSWORD=your_test_password_here
